{
  "name": "sjcdigital",
  "description": "CLI app to scrap and download open data from São José dos Campos",
  "version": "0.0.1",
  "author": {
    "name": "Paulo Luan",
    "email": "pauloluan.inova@gmail.com",
    "url": "github.com/sjcdigital"
  },
  "bin": "dist/cli.js",
  "dependencies": {
    "cheerio": "^0.22.0",
    "cheerio-tableparser": "^1.0.1",
    "lodash": "^4.17.2",
    "meow": "^3.7.0",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-istanbul": "^0.11.0",
    "babel-preset-es2015": "6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "del": "^2.0.2",
    "eslint": "^3.1.1",
    "eslint-config-xo-space": "^0.15.0",
    "eslint-plugin-babel": "^3.3.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.2",
    "gulp-coveralls": "^0.1.0",
    "gulp-eslint": "^3.0.1",
    "gulp-exclude-gitignore": "^1.0.0",
    "gulp-istanbul": "^1.0.0",
    "gulp-line-ending-corrector": "^1.0.1",
    "gulp-mocha": "^3.0.1",
    "gulp-nsp": "^2.1.0",
    "gulp-plumber": "^1.0.0",
    "isparta": "^4.0.0",
    "standard": "^8.6.0"
  },
  "engines": {
    "node": "6.3.1"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "mocha": true
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "http://sjcdigital.com",
  "keywords": [
    "abertos",
    "dados",
    "opendata",
    "sjc"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "PauloLuan/sjcdigital",
  "scripts": {
    "start": "node lib/cli bus",
    "debug": "./node_modules/.bin/node-debug lib/cli bus",
    "build": "./node_modules/.bin/babel src --out-dir dist",
    "coverage": "./node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover _mocha -- --timeout 30000 test/**/*.spec.js",
    "lint": "npm run lint-fix && npm run lint-check",
    "lint-check": "./node_modules/.bin/standard",
    "lint-fix": "./node_modules/.bin/standard --fix lib/**/*.js test/**/*.js",
    "prepublish": "npm run build",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register test/**/*.spec.js",
    "test-debug": "./node_modules/.bin/mocha --watch --compilers js:babel-core/register test/**/*.spec.js --inspect --debug-brk"
  }
}
